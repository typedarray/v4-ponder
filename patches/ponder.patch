+    if (eventValue === undefined)
+        return false;
+    // array
+    if (Array.isArray(filterValue) &&
+        filterValue.some((v) => v === toLowerCase(eventValue))) {
+        return true;
+    }
+    // single
+    if (filterValue === toLowerCase(eventValue))
+        return true;
+    return false;
+};
